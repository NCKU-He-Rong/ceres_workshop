{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug with gdb",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/test_utils",
        "args": [],
        "cwd": "${workspaceFolder}",
        "preLaunchTask": "build (Debug)",
        "stopAtEntry": false,
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          { "description": "Enable pretty printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
        ]
      }
    ]
  }
  